<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="site.hclub.hyndai.mapper.TokenMapper">

 
  <insert id="insertTokenMemberInfo" parameterType="site.hclub.hyndai.domain.JwtToken">
  	INSERT INTO TOKEN 
    	(MEMBER_NO, ACCESS_TOKEN , REFRESH_TOKEN)
	VALUES(#{memberNo},#{accessToken}, #{refreshToken})
  </insert>

</mapper>